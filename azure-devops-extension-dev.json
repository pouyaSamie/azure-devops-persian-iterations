{
    "manifestVersion": 1,
    "id": "PersianIterationManagerHub",
    "publisher": "PouyaSamie",
    "version": "1.0.116",
    "name": "Persian Iteration",
    "description": "Persian Iteration Manager Hub",
    "categories": [
        "Azure Boards",
        "Azure Pipelines"
    ],
    "scopes": [
        "vso.work",
        "vso.work_write"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
